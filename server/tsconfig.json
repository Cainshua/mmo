{
	"compilerOptions": {
		"baseUrl": ".",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["DOM", "ES2015"],
		"module": "commonjs",
		"outDir": "dist",
		"paths": {
			"": ["./*"]
		},
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es5"
	}
}
